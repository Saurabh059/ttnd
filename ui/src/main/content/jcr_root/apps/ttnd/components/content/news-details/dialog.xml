<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <news-details
                jcr:primaryType="cq:Panel"
                title="News Details">
                <items jcr:primaryType="cq:WidgetCollection">
                    <news-location
                        jcr:primaryType="cq:Widget"
                        fieldLabel="News Location"
                        name="./newsLocation"
                        xtype="textfield"/>
                    <news-date
                        jcr:primaryType="cq:Widget"
                        fieldLabel="News Date"
                        name="./newsDate"
                        xtype="datetime"/>
                    <author
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Author"
                        name="./author"
                        xtype="textfield"/>
                </items>
            </news-details>
        </items>
    </items>
</jcr:root>
